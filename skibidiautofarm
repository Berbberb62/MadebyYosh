local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Create the initial password ScreenGui
local PasswordGui = Instance.new("ScreenGui")
PasswordGui.Name = "PasswordGUI"
PasswordGui.Parent = playerGui

local PasswordFrame = Instance.new("Frame")
PasswordFrame.Size = UDim2.new(0, 250, 0, 100)
PasswordFrame.Position = UDim2.new(0.5, -150, 0.5, -50)
PasswordFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- Set to black
PasswordFrame.BorderSizePixel = 0
PasswordFrame.Active = true
PasswordFrame.Draggable = true
PasswordFrame.Parent = PasswordGui

-- Add corner radius to the frame
local cornerPasswordFrame = Instance.new("UICorner")
cornerPasswordFrame.CornerRadius = UDim.new(0, 10)
cornerPasswordFrame.Parent = PasswordFrame

local PasswordLabel = Instance.new("TextLabel")
PasswordLabel.Size = UDim2.new(1, -20, 0, 30)
PasswordLabel.Position = UDim2.new(0, 10, 0, 10)
PasswordLabel.BackgroundTransparency = 1
PasswordLabel.Text = "Made By Berun"
PasswordLabel.TextColor3 = Color3.fromRGB(191, 0, 230) -- White text
PasswordLabel.Font = Enum.Font.SourceSans
PasswordLabel.TextSize = 18
PasswordLabel.TextXAlignment = Enum.TextXAlignment.Left
PasswordLabel.Parent = PasswordFrame

local PasswordInput = Instance.new("TextBox")
PasswordInput.Size = UDim2.new(0.7, -20, 0, 30) -- Reduced width for space
PasswordInput.Position = UDim2.new(0, 10, 0.5, -15)
PasswordInput.BackgroundColor3 = Color3.fromRGB(0, 0, 0) -- Black background
PasswordInput.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text
PasswordInput.Text = "" -- No text inside the textbox by default
PasswordInput.PlaceholderText = "Enter password"
PasswordInput.PlaceholderColor3 = Color3.fromRGB(150, 150, 150) -- Gray placeholder
PasswordInput.Font = Enum.Font.SourceSans
PasswordInput.TextSize = 18
PasswordInput.ClearTextOnFocus = false
PasswordInput.BorderSizePixel = 0
PasswordInput.Parent = PasswordFrame

-- Add corner radius to the textbox
local cornerPasswordInput = Instance.new("UICorner")
cornerPasswordInput.CornerRadius = UDim.new(0, 10)
cornerPasswordInput.Parent = PasswordInput

local SubmitButton = Instance.new("TextButton")
SubmitButton.Size = UDim2.new(0.25, 0, 0, 30) -- Adjusted width for the button
SubmitButton.Position = UDim2.new(0.65, 10, 0.5, -15) -- Positioned on the right side with padding
SubmitButton.BackgroundColor3 = Color3.fromRGB(191, 0, 230) -- Green for visibility
SubmitButton.Text = "Submit"
SubmitButton.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text
SubmitButton.Font = Enum.Font.SourceSans
SubmitButton.TextSize = 18
SubmitButton.Parent = PasswordFrame

-- Add corner radius to the submit button
local cornerSubmitButton = Instance.new("UICorner")
cornerSubmitButton.CornerRadius = UDim.new(0, 10)
cornerSubmitButton.Parent = SubmitButton

-- Function to handle password input
SubmitButton.MouseButton1Click:Connect(function()
    if PasswordInput.Text == "berun" then
        PasswordGui:Destroy() -- Remove the password GUI
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Berbberb62/Obfuscated/refs/heads/main/skibidiautofarm"))()
        -- Insert your main GUI setup code here to continue the script
        
    else
        PasswordInput.Text = "" -- Clear input if password is wrong
        PasswordInput.PlaceholderText = "Incorrect, try again"
    end
end)
